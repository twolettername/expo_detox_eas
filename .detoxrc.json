{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "skipLegacyWorkersInjection": true,
  "apps": {
    "ios:debug": {
      "type": "ios.app",
      "binaryPath": "e2e/builds/ios.app",
      "build": "eas build --local --platform ios --profile e2e:debug --non-interactive --output e2e/builds/ios.tar.gz && tar -xf e2e/builds/ios.tar.gz"
    },
    "android:debug": {
      "type": "android.apk",
      "binaryPath": "e2e/builds/android.apk",
      "build": "expo prebuild --clean -p android && eas build --local --platform android --profile e2e:debug --non-interactive --output e2e/builds/android.apk"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 11"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "Pixel_3a_API_30_x86"
      }
    }
  },
  "configurations": {
    "ios:debug": {
      "device": "simulator",
      "app": "ios:debug"
    },
    "android:debug": {
      "device": "emulator",
      "app": "android:debug"
    }
  }
}